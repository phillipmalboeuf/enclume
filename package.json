{
  "name": "enclume",
  "version": "1.0.0",
  "engines": {
    "node": "11.10"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "app": "parcel index.html -p 8080 --open",
    "server": "parcel watch server/server.tsx --target=node --out-file=server.js | nodemon dist/server.js",
    "build": "parcel build app/app.tsx --out-file=app.js --no-cache --no-source-maps && parcel build server/server.tsx --target=node --out-file=server.js --no-source-maps --no-cache",
    "build:app": "parcel build app/app.tsx --out-file=app.js"
  },
  "author": "Phillip Malboeuf <phillip.malboeuf@gmail>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "browser-cookies": "^1.2.0",
    "compression": "^1.7.3",
    "contentful": "^7.3.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dompurify": "^1.0.10",
    "express": "^4.16.4",
    "express-useragent": "^1.0.12",
    "headroom.js": "^0.9.4",
    "jsonschema": "^1.2.4",
    "mongodb": "^3.1.13",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "object-path": "^0.11.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "react-spring": "^8.0.8"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dompurify": "0.0.32",
    "@types/express": "^4.16.1",
    "@types/express-useragent": "^0.2.21",
    "@types/jsonschema": "^1.1.1",
    "@types/mongodb": "^3.1.19",
    "@types/morgan": "^1.7.35",
    "@types/nconf": "^0.10.0",
    "@types/object-path": "^0.11.0",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.17.2",
    "typescript": "^3.3.3333"
  }
